<Window x:Name="window" x:Class="TVGL_Presenter.Window2DPlot"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:oxy="http://oxyplot.org/wpf"
        xmlns:local="clr-namespace:TVGL_Presenter"
         Height="350" Width="525">
    <Grid>
        <oxy:Plot Title="{Binding Title, ElementName=window}" x:Name="Plot" />
    </Grid>
</Window>

