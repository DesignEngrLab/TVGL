<Window x:Name="window" x:Class="TVGL.Window2DPlot"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="http://oxyplot.org/wpf"
        Title="Presenter 2D" Height="480" Width="640">
    <Grid>
        <!-- The OxyPlot control is binding to a PlotModel in the MainViewModel -->
        <oxy:PlotView  Model="{Binding Model, ElementName=window}" />


    </Grid>
</Window>
